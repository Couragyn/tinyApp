<!-- view for listing all URLs in dic -->
<!DOCTYPE html>
<html lang="en">
<head>
</head>
  <body>
  <% include partials/__header %>
  <h1>URL List</h1>
  <table style="width:100%">
    <% for (prop in urls) { %>
      <% if (user === urls[prop].user_id) { %>
        <tr>
          <td><%= prop%></td>
          <td><%= urls[prop].longURL %></td>
          <td><a href="urls/<%= prop %>">Edit</a></td>
          <td>
            <form action='/urls/<%= prop %>/delete' method='POST'>
              <button>Delete</button>
            </form>
          </td>
        </tr>
      <% }; %>
    <% }; %>
  </table>
  <a href="./urls/new">Create a New Short Link</a>
  </body>
</html>
