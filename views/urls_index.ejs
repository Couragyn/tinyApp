<!-- view for listing all URLs in dic -->
<!DOCTYPE html>
<html lang="en">
<head>
</head>
<body>
  <ol>
    <h1>URL List</h1>
    <% include partials/__header %>
    <% for (prop in urls) { %>
    <form action='/urls/<%= prop %>/delete' method='post'>
        <li> <%= urls[prop] %> - <%= prop %>
          <a href="urls/<%= prop %>">edit</a>
            <button>Delete</button>
        </li>
    </form>

      <% }; %>
  </ol>
    <a href="./urls/new">New shorten URL</a>
</body>
</html>
